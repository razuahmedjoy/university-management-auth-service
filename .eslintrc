{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  // HERE
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],

  "rules": {
    "no-unused-vars": "warn",
    // "no-console":"warn",
    "no-undef": "warn",
    "no-unused-expressions": "warn",
    "no-unreachable": "error"
    // "consistent-type-definitions": ["warn", "type"]
  },

  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals":{
    "process": "readonly"
  }
}
